all: 		clean EXE___16.OBJ EXE___32.OBJ SEKT__16.OBJ SEKT__32.OBJ DAT___16.OBJ DAT___32.OBJ

clean:
	rem for %%d in ( *.obj exe.dat sekt.dat dat.dat ) do if exist %%d del %%d
	cls

EXE___16.OBJ: 	EXE.DAT
	C:\BP\BIN\BINOBJ EXE.DAT EXE___16.OBJ EXE_BUSCH

EXE___32.OBJ: 	EXE.DAT
	BINOBJ32 EXE.DAT EXE___32.OBJ EXE_BUSCH

SEKT__16.OBJ: 	SEKT.DAT
	C:\BP\BIN\BINOBJ SEKT.DAT SEKT__16.OBJ SEKT_BUSCH 

SEKT__32.OBJ: 	SEKT.DAT
	BINOBJ32 SEKT.DAT SEKT__32.OBJ SEKT_BUSCH
        
DAT___16.OBJ: 	DAT.DAT
	C:\BP\BIN\BINOBJ DAT.DAT DAT___16.OBJ DAT_BUSCH

DAT___32.OBJ: 	DAT.DAT
	BINOBJ32 DAT.DAT DAT___32.OBJ DAT_BUSCH
        


SEKT.DAT: 	BUS_SEKT.PAS BUSCHBAU.PAS BUSCHSUC.PAS
	PASVP BUS_SEKT .\ /U.. /I..
	BUS_SEKT.EXE
	DEL BUS_SEKT.EXE

EXE.DAT:	BUS_EXE0.PAS BUS_EXE2.PAS BUSCHBAU.PAS BUSCHSUC.PAS
	PASVP BUS_EXE0 .\ /U.. /I..
	BUS_EXE0.EXE
	DEL BUS_EXE0.EXE

DAT.DAT: 	BUS_DAT.PAS BUSCHBAU.PAS BUSCHSUC.PAS
	PASVP BUS_DAT .\ /U.. /I..
	BUS_DAT.EXE
	DEL BUS_DAT.EXE
