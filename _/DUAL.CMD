@ECHO OFF
REM
REM CALL PASBP %1 %2     %3 %4 %5 %6 %7 %8 %9
REM CALL PASVP %1 %2     %3 %4 %5 %6 %7 %8 %9  
REM CALL LXLITE ...
REM
IF NOT EXIST %1.PAS GOTO FEHLER
IF [%TMP%]==[] GOTO FEHLER

IF EXIST %2%1.EXE DEL %2%1.EXE
IF EXIST %2%1.OS2 DEL %2%1.OS2
IF EXIST %2%1.DOS DEL %2%1.DOS
IF EXIST %TMP%\%1.EXE DEL %TMP%\%1.EXE
IF EXIST %TMP%\%1.OS2 DEL %TMP%\%1.OS2
IF EXIST %TMP%\%1.DOS DEL %TMP%\%1.DOS

CALL PASVP.CMD %1 %TMP%\ %3 %4 %5 %6 %7 %8 %9
IF NOT EXIST %TMP%\%1.EXE GOTO FEHLER
REN %TMP%\%1.EXE *.OS2
CALL PASBP.BAT %1 %TMP%\ %3 %4 %5 %6 %7 %8 %9
IF NOT EXIST %TMP%\%1.EXE GOTO FEHLER
REN %TMP%\%1.EXE *.DOS

CALL LX.CMD %TMP%\%1.OS2 /T:%TMP%\%1.DOS /ZS0
SIEGEL.EXE %TMP%\%1.OS2
DEL %TMP%\%1.DOS

IF     [%TMP%\]==[%2] REN %2%1.OS2 *.EXE
IF NOT [%TMP%\]==[%2] COPY %TMP%\%1.OS2 %2%1.EXE
IF NOT [%TMP%\]==[%2] DEL  %TMP%\%1.OS2

CALL TBSX %2%1.EXE
GOTO ENDE

:FEHLER
ECHO DUAL %1,%2
PAUSE

:ENDE
