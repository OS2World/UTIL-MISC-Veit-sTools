@ECHO OFF
IF [%TMP%]==[] GOTO FEHLER
CALL A86COMDE ITAB_DAT FB_MENU.VK\
CALL A86COMDE BIOS_DAT FB_MENU.VK\

CALL A86BIN MENU_DEU %TMP%\
IF NOT EXIST %TMP%\MENU_DEU.BIN GOTO FEHLER
CALL A86BIN MENU_ENG %TMP%\
SCHNITT %TMP%\MENU_DEU.BIN $DC80 %TMP%\MENU_DEU.DC8
SCHNITT %TMP%\MENU_ENG.BIN $DC80 %TMP%\MENU_ENG.DC8
DEL %TMP%\MENU_DEU.BIN
DEL %TMP%\MENU_ENG.BIN
BINOBJ %TMP%\MENU_DEU.DC8 .\MENU_DEU.OBJ MENU_KODE_DEU
BINOBJ %TMP%\MENU_ENG.DC8 .\MENU_ENG.OBJ MENU_KODE_ENG
DEL %TMP%\MENU_DEU.DC8
DEL %TMP%\MENU_ENG.DC8

CALL A86BIN LADE_DEU %TMP%\
IF NOT EXIST %TMP%\LADE_DEU.BIN GOTO FEHLER
CALL A86BIN LADE_ENG %TMP%\
SCHNITT %TMP%\LADE_DEU.BIN $8000 %TMP%\LADE_DEU.800
SCHNITT %TMP%\LADE_ENG.BIN $8000 %TMP%\LADE_ENG.800
DEL %TMP%\LADE_DEU.BIN
DEL %TMP%\LADE_ENG.BIN
BINOBJ %TMP%\LADE_DEU.800 .\LADE_DEU.OBJ LADE_KODE_DEU
BINOBJ %TMP%\LADE_ENG.800 .\LADE_ENG.OBJ LADE_KODE_ENG
DEL %TMP%\LADE_DEU.800
DEL %TMP%\LADE_ENG.800

CALL PASBP FB_M_DEF %TMP%\
%TMP%\FB_M_DEF.EXE
DEL %TMP%\FB_M_DEF.EXE
CALL PASBP FB_MENU FB_MENU.VK\
CALL ..\GENVK FB_MENU
GOTO ENDE

:FEHLER
ECHO FEHLER !
PAUSE

:ENDE
