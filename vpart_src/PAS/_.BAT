@ECHO OFF

ECHO þ VPartDOS
CALL PASBP VPARTDEF %TMP%\
IF NOT EXIST %TMP%\VPARTDEF.EXE GOTO FEHLER
%TMP%\VPARTDEF.EXE
DEL %TMP%\VPARTDEF.EXE
REM [sp„ter] COPY VPARTEIN.SPR ..\VPART.VK\VPARTEIN.SPR

CALL PASBP VPARTEIN ..\VPART.VK\ /UTP /U\UNIT\SPRACHE2 /ITP /I\UNIT\SPRACHE2
IF NOT EXIST ..\VPART.VK\VPARTEIN.EXE GOTO FEHLER
CD ..\VPART.VK
IF EXIST VPARTDOS.EXE DEL VPARTDOS.EXE
REN VPARTEIN.EXE VPARTDOS.EXE
CD ..\PAS

ECHO þ SetBootC
CALL PASBP SETB_DEF %TMP%\
IF NOT EXIST %TMP%\SETB_DEF.EXE GOTO FEHLER
%TMP%\SETB_DEF.EXE
DEL %TMP%\SETB_DEF.EXE

IF EXIST SETBOOTC.COM DEL SETBOOTC.COM
IF EXIST SETBOOTF.COM DEL SETBOOTF.COM
CALL PASBP SETBOOTC .\ /UTP /I\SPRACHE2 /U\SPRACHE2
CALL PASBP SETBOOTF .\ /UTP /I\SPRACHE2 /U\SPRACHE2 
REN SETBOOTC.EXE SETBOOTC.COM
REN SETBOOTF.EXE SETBOOTF.COM
DEL ANTI-VIR.DAT

GOTO ENDE

:FEHLER
