@ECHO OFF

rem IF EXIST vpart$$$.001 GOTO SPR2
CALL PASVP VPARTDEF %TMP%\ @VPARTOS2.CFG
IF NOT EXIST %TMP%\VPARTDEF.EXE GOTO FEHLER
%TMP%\VPARTDEF.EXE
DEL %TMP%\VPARTDEF.EXE
:SPR2

ECHO þ VPartOS2
CALL PASVP VPARTEIN ..\VPART.VK\ @VPARTOS2.CFG
IF NOT EXIST ..\VPART.VK\VPARTEIN.EXE GOTO FEHLER
CD ..\VPART.VK
IF EXIST VPARTOS2.EXE DEL VPARTOS2.EXE
REN VPARTEIN.EXE VPARTOS2.EXE
CD ..\PAS

ECHO þ VPartD32
CALL PASVPDSP VPARTEIN ..\VPART.VK\ @VPARTD32.CFG
IF NOT EXIST ..\VPART.VK\VPARTEIN.EXE GOTO FEHLER
CD ..\VPART.VK
IF EXIST VPARTD32.EXE DEL VPARTD32.EXE
REN VPARTEIN.EXE VPARTD32.EXE
COPY C:\EXTRA\WDOSX\WDOSXLE.EXE .\
CD ..\PAS

ECHO þ SetBootC
CALL PASVP SETBOOTC ..\VPART.VK\ /UOS2 /I\SPRACHE2 /U\SPRACHE2
CALL PASVP SETBOOTF ..\VPART.VK\ /UOS2 /I\SPRACHE2 /U\SPRACHE2 
CALL LXLITE ..\VPART.VK\SETBOOTC.EXE /U+ /F+ /ZS /T:SETBOOTC.COM
CALL LXLITE ..\VPART.VK\SETBOOTF.EXE /U+ /F+ /ZS /T:SETBOOTF.COM
DEL SETBOOTC.COM
DEL SETBOOTF.COM
IF EXIST ANTI-VIR.DAT DEL ANTI-VIR.DAT
CALL SIEGEL ..\VPART.VK\SETBOOTC.EXE
CALL SIEGEL ..\VPART.VK\SETBOOTF.EXE

GOTO ENDE

:FEHLER
ECHO *FEHLER*
SET FEHLER=1
:ENDE
